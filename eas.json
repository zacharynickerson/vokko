{
  "cli": {
    "version": ">= 10.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "v20.9.0",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.9.0",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "node": "v20.9.0",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "OPENAI_API_KEY": "OPENAI_API_KEY"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
